<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context='.Repair.eqrepiaraccept'>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="110dp"
        android:id="@+id/eqreapireqaccept_mainheader"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:background="#bec4bf"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#5067c6"
            android:layout_margin="5dp"
            android:padding="10dp"
            android:id="@+id/eqreapireqaccept_mainheaderll1"
            >
            <TextView
                android:layout_width="300dp"
                android:layout_height="25dp"
                android:text="編號信息區："
                android:gravity="center_vertical"
                android:id="@+id/eqreapireqaccept_mainheaderlabel"
                android:textSize="20dp"
                android:textIsSelectable="false"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="3dp"
                android:textColor="#ffffff"
                ></TextView>
            </LinearLayout>

        <TextView
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:text="SYS ID:"
            android:gravity="center_vertical"
            android:id="@+id/eqreapireqaccept_mainheaderctv1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/eqreapireqaccept_mainheaderll1"
            android:layout_alignParentStart="true"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />

        <TextView
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center_vertical"
            android:id="@+id/eqreapireqaccept_mainheadercet1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/eqreapireqaccept_mainheaderll1"
            android:layout_toEndOf="@+id/eqreapireqaccept_mainheaderctv1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            >

        </TextView>

        <Button
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:gravity="center_vertical"
            android:text="提交"
            android:id="@+id/eqreapireqaccept_mainheadercb3"
            android:background="@drawable/btn_dl"
            android:textAlignment="center"
            android:textSize="15dp"
            android:layout_below="@+id/eqreapireqaccept_mainheaderll1"
            android:layout_toEndOf="@+id/eqreapireqaccept_mainheadercet1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="10dp"
            android:onClick="onuploadeqcheckdata"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="800dp"
        android:id="@+id/eqreapireqaccept_maindata"
        android:layout_below="@+id/eqreapireqaccept_mainheader"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:background="#bec4bf"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#5067c6"
            android:layout_margin="5dp"
            android:padding="10dp"
            android:id="@+id/eqreapireqaccept_maindatahll1"
            >
            <TextView
            android:layout_width="300dp"
            android:layout_height="25dp"
            android:text="調整項目"
            android:gravity="center_vertical"
            android:id="@+id/eqreapireqaccept_maindatadhtv1"
            android:textSize="20dp"
            android:textIsSelectable="false"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            android:textColor="#ffffff"
            ></TextView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:id="@+id/eqreapireqaccept_maindatadhll2"
            android:layout_below="@+id/eqreapireqaccept_maindatahll1"
            >

            <TextView
                android:id="@+id/listviewsource11Mname0"
                android:layout_width="30dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:background="#fded6f"
                android:textColor="#000000"
                android:textSize="13sp"
                android:text="調整項目"
                android:layout_alignParentStart="true"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="1dp"
                />
        <TextView
            android:id="@+id/listviewsource11Mname1"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:background="#abf6a8"
            android:textColor="#000000"
            android:textSize="13sp"
            android:text="驗收項目ID"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/listviewsource11Mname0"
            />
        <TextView
            android:id="@+id/listviewsource11Mname2"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:text="驗收項目"
            android:textSize="13sp"
            android:background="#e8efaf"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/listviewsource11Mname1"
            />
        <TextView
            android:id="@+id/listviewsource11Mname3"
            android:layout_width="30dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:text="所有者"
            android:textSize="13sp"
            android:background="#a5dc88"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/listviewsource11Mname2"
            />
        <TextView
            android:id="@+id/listviewsource11Mname4"
            android:layout_width="30dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:text="執行模式"
            android:textSize="13sp"
            android:background="#cfcbc1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/listviewsource11Mname3"
            />

        <TextView
            android:id="@+id/listviewsource11Mname5"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:text="值"
            android:textSize="13sp"
            android:background="#f7d37e"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/listviewsource11Mname4"
            />
            
</LinearLayout>
        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:id="@+id/eqreapireqaccept_maindatadsv1"
            android:layout_below="@+id/eqreapireqaccept_maindatadhll2">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:orientation="vertical"
                android:id="@+id/eqreapireqaccept_maindataddll3"

                >
            </LinearLayout>

        </ScrollView>

        </RelativeLayout>

</RelativeLayout>
