<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".IncludingSFC.FOLInOutCheckGAOven">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="150dp"
        android:id="@+id/isfc_folinoutcheckgaoven_headerrl1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:background="#bec4bf"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#5067c6"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:id="@+id/isfc_folinoutcheckgaoven_headerll1"
            >
            <TextView
                android:layout_width="300dp"
                android:layout_height="25dp"
                android:text="基礎信息:"
                android:gravity="center_vertical"
                android:id="@+id/isfc_folinoutcheckgaoven_mainlabelheader"
                android:textSize="20dp"
                android:textIsSelectable="false"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="3dp"
                android:textColor="#ffffff"
                ></TextView>

            <Button
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:gravity="center_vertical"
                android:text="提交"
                android:id="@+id/isfc_folinoutcheckgaoven_submitbt"
                android:background="@drawable/btn_dl"
                android:textAlignment="center"
                android:textSize="15dp"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_mainlabelheader"
                android:layout_marginLeft="20dp"
                android:onClick="csubmitclick"/>

        </LinearLayout>

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:text="批號:"
            android:gravity="center_vertical"
            android:textAlignment="textEnd"
            android:id="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_headerll1"
            android:layout_alignParentStart="true"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />
        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:text="XXXXXXXX"
            android:gravity="center_vertical"
            android:id="@+id/isfc_folinoutcheckgaoven_lotnotbv1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_headerll1"
            android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />
        <TextView
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text="數量:"
            android:gravity="center_vertical"
            android:textAlignment="textEnd"
            android:id="@+id/isfc_folinoutcheckgaoven_dieqtytbl1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_headerll1"
            android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_lotnotbv1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />
        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:text="XXXXXXXX"
            android:gravity="center_vertical"
            android:id="@+id/isfc_folinoutcheckgaoven_dieqtytbv1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_headerll1"
            android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_dieqtytbl1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />

        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:text="當前站位:"
            android:gravity="center_vertical"
            android:textAlignment="textEnd"
            android:id="@+id/isfc_folinoutcheckgaoven_opnotbl1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:layout_alignParentStart="true"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />
        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:text="XXXXXXXX"
            android:gravity="center_vertical"
            android:id="@+id/isfc_folinoutcheckgaoven_opnotbv1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />

        <TextView
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:textAlignment="textEnd"
            android:text="當前站位名稱:"
            android:gravity="center_vertical"
            android:id="@+id/isfc_folinoutcheckgaoven_opnamel1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_opnotbv1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />

        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:text="XXXXXXXX"
            android:gravity="center_vertical"
            android:id="@+id/isfc_folinoutcheckgaoven_opnametv1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_lotnotbl1"
            android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_opnamel1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="3dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="110dp"
        android:id="@+id/isfc_folinoutcheckgaoven_canrl1"
        android:layout_below="@+id/isfc_folinoutcheckgaoven_headerrl1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:background="#bec4bf"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#5067c6"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:id="@+id/isfc_folinoutcheckgaoven_scanll1"
            >
            <TextView
                android:layout_width="300dp"
                android:layout_height="25dp"
                android:text="掃描區："
                android:gravity="center_vertical"
                android:id="@+id/isfc_folinoutcheckgaoven_scantl1"
                android:textSize="20dp"
                android:textIsSelectable="false"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="3dp"
                android:textColor="#ffffff"
                ></TextView>
        </LinearLayout>
        <EditText
            android:layout_width="400dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center_vertical"
            android:id="@+id/isfc_folinoutcheckgaoven_scanet1"
            android:textSize="16dp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_scanll1"
            android:layout_alignParentStart="true"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="150dp"
            android:background="#ffffff"
            android:onClick="scantbclick"
            />
    </RelativeLayout>
    <!-- 備平資料選擇區-->
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="410dp"
        android:id="@+id/isfc_folinoutcheckgaoven_stationprl1"
        android:layout_below="@+id/isfc_folinoutcheckgaoven_canrl1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:background="#bec4bf"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#5067c6"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:id="@+id/isfc_folinoutcheckgaoven_stationpll1"
            >
            <TextView
                android:layout_width="300dp"
                android:layout_height="25dp"
                android:text="不良原因輸入："
                android:gravity="center_vertical"
                android:id="@+id/isfc_folinoutcheckgaoven_stationptl1"
                android:textSize="20dp"
                android:textIsSelectable="false"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="3dp"
                android:textColor="#ffffff"
                ></TextView>
        </LinearLayout>


        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:id="@+id/isfc_folinoutcheckgaoven_stationpheaderll1"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_stationpll1"
            android:layout_marginTop="3dp"
            >
            <TextView
                android:id="@+id/isfc_folinoutcheckgaoven_stationpheadertvl1"
                android:layout_width="70dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:background="#abf6a8"
                android:textSize="15sp"
                android:text="批號"
                android:textAlignment="center"
                android:layout_alignParentStart="true"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="1dp"
                />
            <TextView
                android:id="@+id/isfc_folinoutcheckgaoven_stationpheadertvl2"
                android:layout_width="70dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:textSize="15sp"
                android:text="站位"
                android:textAlignment="center"
                android:background="#e8efaf"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="1dp"
                android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_stationpheadertvl1"
                />
            <TextView
                android:id="@+id/isfc_folinoutcheckgaoven_stationpheadertvl3"
                android:layout_width="70dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:textSize="15sp"
                android:text="彈夾編號"
                android:textAlignment="center"
                android:background="#a5dc88"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="1dp"
                android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_stationpheadertvl2"
                />

            <TextView
                android:id="@+id/isfc_folinoutcheckgaoven_stationpheadertvl4"
                android:layout_width="70dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:textSize="15sp"
                android:text="掃描人員"
                android:textAlignment="center"
                android:background="#5adac9"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="1dp"
                android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_stationpheadertvl3"
                />

            <TextView
                android:id="@+id/isfc_folinoutcheckgaoven_stationpheadertvl5"
                android:layout_width="70dp"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:textSize="15sp"
                android:text="掃描IP"
                android:textAlignment="center"
                android:background="#c000ff00"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="1dp"
                android:layout_toEndOf="@+id/isfc_folinoutcheckgaoven_stationpheadertvl4"
                />
        </LinearLayout>

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/isfc_folinoutcheckgaoven_stationpheaderll1">
            <LinearLayout
                android:id="@+id/isfc_folinoutcheckgaoven_stationviewll1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="3dp"
                android:orientation="vertical"
                >
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>


</RelativeLayout>
