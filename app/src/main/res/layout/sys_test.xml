<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:padding="16dp"
    tools:context=".eqcheck">

    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata1et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        />

    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata2et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_below="@+id/systest_indata1et"
        android:layout_alignParentStart="true"
        />

    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata3et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_below="@+id/systest_indata2et"
        android:layout_alignParentStart="true"
        />

    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata4et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_below="@+id/systest_indata3et"
        android:layout_alignParentStart="true"
        />

    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata5et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_below="@+id/systest_indata4et"
        android:layout_alignParentStart="true"
        />

    <Button
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:text="创建"
        android:id="@+id/systest_indata1bt"
        android:background="@drawable/btn_dl"
        android:textAlignment="center"
        android:textSize="15dp"
        android:layout_below="@+id/systest_indata5et"
        android:layout_toEndOf="@+id/systest_indata5et"
        android:layout_marginLeft="1dp"
        android:onClick="sysbtsubmit"/>

    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata6et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_below="@+id/systest_indata1bt"
        android:layout_alignParentStart="true"
        />
    <Spinner
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="机种："
        android:gravity="center_vertical"
        android:id="@+id/systest_indata7et"
        android:textSize="16dp"
        android:textIsSelectable="false"
        android:textAlignment="center"
        android:layout_below="@+id/systest_indata6et"
        android:layout_alignParentStart="true"
        />
    <Button
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:text="创建"
        android:id="@+id/systest_indata2bt"
        android:background="@drawable/btn_dl"
        android:textAlignment="center"
        android:textSize="15dp"
        android:layout_below="@+id/systest_indata7et"
        android:layout_toEndOf="@+id/systest_indata7et"
        android:layout_marginLeft="1dp"
        android:onClick="sysbtsubmittest"/>

</RelativeLayout>
