<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Reserve.rsstrocklist">
    <LinearLayout android:id="@+id/rs_strocklist_loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
    </LinearLayout>
    <LinearLayout
        android:id="@+id/rs_strocklistheader_ll1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        >
        <TextView
            android:id="@+id/rs_strocklistheader_tv1"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="部件編碼"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:background="#abf6a8"
            android:textSize="15sp"
            android:layout_alignParentStart="true"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            />
        <TextView
            android:id="@+id/rs_strocklistheader_tv2"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="品名"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#e8efaf"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv1"
            />
        <TextView
            android:id="@+id/rs_strocklistheader_tv3"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="料號"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#a5dc88"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv2"
            />
        <TextView
            android:id="@+id/rs_strocklistheader_tv4"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="規格"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#cfcbc1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv3"
            />

        <TextView
            android:id="@+id/rs_strocklistheader_tv5"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="機臺"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#f7d37e"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv4"
            />

        <TextView
            android:id="@+id/rs_strocklistheader_tv6"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="站位"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#84f4d1"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv5"
            />

        <TextView
            android:id="@+id/rs_strocklistheader_tv7"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="庫存"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#a9aeac"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv6"
            />
        <TextView
            android:id="@+id/rs_strocklistheader_tv8"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="安全庫存"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#7ffa76"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv7"
            />
        <TextView
            android:id="@+id/rs_strocklistheader_tv9"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="廠商"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#e7ef77"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv8"
            />
        <TextView
            android:id="@+id/rs_strocklistheader_tv10"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="廠區"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#dfb664"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv9"
            />

        <TextView
            android:id="@+id/rs_strocklistheader_tv11"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="車間"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#c9caaf"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv10"
            />

        <TextView
            android:id="@+id/rs_strocklistheader_tv12"
            android:layout_width="70dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="樓層"
            android:gravity="center_vertical"
            android:textColor="#000000"
            android:textSize="15sp"
            android:background="#b1ef8a"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            android:layout_toEndOf="@+id/rs_strocklistheader_tv11"
            />


    </LinearLayout>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strocklistheader_ll1"
        >

        <LinearLayout
            android:id="@+id/rs_strocklist_ll1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

        </LinearLayout>

    </ScrollView>



</RelativeLayout>
