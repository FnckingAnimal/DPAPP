<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                tools:context=".IncludingSFC.geterpissueorderdetail">

    <RelativeLayout
        android:id="@+id/isfc_geterpissueorderdetail_hrl"
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="3dp"
        android:background="#bec4bf"
        >

        <LinearLayout
            android:id="@+id/isfc_geterpissueorderdetail_hll1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="#5067c6"
            android:padding="5dp"
            >

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_htv1"
                android:layout_width="300dp"
                android:layout_height="25dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:gravity="center_vertical"
                android:text="發料單"
                android:textColor="#ffffff"
                android:textIsSelectable="false"
                android:textSize="20dp"
                ></TextView>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/isfc_geterpissueorderdetail_hll3"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_below="@+id/isfc_geterpissueorderdetail_hll1"
            android:layout_margin="5dp"
            android:padding="5dp"
            >


            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernolv1"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/isfc_mamaterialcall_hll1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:gravity="center_vertical"
                android:text="發料單號:"
                android:textAlignment="center"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernotv1"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_ordernolv1"
                android:gravity="center_vertical"
                android:text="XXXXXXX"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernolv2"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_ordernotv1"
                android:gravity="center_vertical"
                android:text="機台號:"
                android:textAlignment="center"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernotv2"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:layout_below="@+id/isfc_mamaterialcall_eqsysidtv1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_ordernolv2"
                android:gravity="center_vertical"
                android:text="XXXXXXX"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernolv3"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/isfc_geterpissueorderdetail_ordernolv1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:gravity="center_vertical"
                android:text="發料日期:"
                android:textAlignment="center"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernotv3"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:layout_below="@+id/isfc_geterpissueorderdetail_ordernotv1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_ordernolv3"
                android:gravity="center_vertical"
                android:text="XXXXXXX"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernolv4"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:layout_below="@+id/isfc_geterpissueorderdetail_ordernotv1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_ordernotv3"
                android:gravity="center_vertical"
                android:text="批號:"
                android:textAlignment="center"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

            <TextView
                android:id="@+id/isfc_geterpissueorderdetail_ordernotv4"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:layout_below="@+id/isfc_geterpissueorderdetail_ordernotv1"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_ordernolv4"
                android:gravity="center_vertical"
                android:text="XXXXXXX"
                android:textIsSelectable="false"
                android:textSize="16dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/isfc_geterpissueorderdetail_hll2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/isfc_geterpissueorderdetail_hll3"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            >

            <LinearLayout
                android:id="@+id/isfc_geterpissueorderdetail_listhll2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                >

                <TextView
                    android:id="@+id/isfc_geterpissueorderdetail_dhtv1"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:background="#12CdFf"
                    android:gravity="center_vertical"
                    android:text="sysid"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    />

                <TextView
                    android:id="@+id/isfc_geterpissueorderdetail_dhtv2"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_dhtv1"
                    android:layout_weight="1"
                    android:background="#a2edaf"
                    android:gravity="center_vertical"
                    android:text="物料名稱"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    />

                <TextView
                    android:id="@+id/isfc_geterpissueorderdetail_dhtv3"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_dhtv2"
                    android:layout_weight="1"
                    android:background="#e8efaf"
                    android:gravity="center_vertical"
                    android:text="物料批號"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    />

                <TextView
                    android:id="@+id/isfc_geterpissueorderdetail_dhtv6"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_dhtv5"
                    android:layout_weight="1"
                    android:background="#d5b0f5"
                    android:gravity="center_vertical"
                    android:text="發料量"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    />

                <TextView
                    android:id="@+id/isfc_geterpissueorderdetail_dhtv7"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="3dp"
                    android:layout_toEndOf="@+id/isfc_geterpissueorderdetail_dhtv6"
                    android:layout_weight="1"
                    android:background="#5fc9d9"
                    android:gravity="center_vertical"
                    android:text="實發量"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    />
            </LinearLayout>

            <ScrollView
                android:id="@+id/isfc_geterpissueorderlist_dss1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/isfc_geterpissueorderdetail_listhll2"
                android:layout_marginTop="2dp">

                <LinearLayout
                    android:id="@+id/isfc_geterpissueorderdetail_dll1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                </LinearLayout>

            </ScrollView>
        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:layout_below="@id/isfc_geterpissueorderdetail_hrl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal">
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="60dp"
            android:background="@drawable/btn_dl"
            android:layout_marginTop="20dp"
            android:onClick="listview13_bt_onClick_shouliao"
            android:text="收料"
            android:textAlignment="center"
            android:textSize="15dp" />

    </LinearLayout>



</RelativeLayout>
