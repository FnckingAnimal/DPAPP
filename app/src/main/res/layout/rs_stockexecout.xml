<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Reserve.rsstrockexeco">

    <TextView
        android:id="@+id/rs_strockexeco_tv1"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="班別:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        ></TextView>
    <Spinner
        android:id="@+id/rs_strockexeco_shiftsp"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/rs_strockexeco_tv1"
        >
    </Spinner>

    <TextView
        android:id="@+id/rs_strockexeco_tv2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="車間:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_shiftsp"
        android:layout_alignParentLeft="true"
        ></TextView>
    <TextView
        android:id="@+id/rs_strockexeco_areatv"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text=""
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_shiftsp"
        android:layout_toEndOf="@+id/rs_strockexeco_tv2"
        >
    </TextView>

    <TextView
        android:id="@+id/rs_strockexeco_tv3"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="樓層區域:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv2"
        android:layout_alignParentLeft="true"
        ></TextView>
    <TextView
        android:id="@+id/rs_strockexeco_floortv"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text=""
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_areatv"
        android:layout_toEndOf="@+id/rs_strockexeco_tv3"
        >
    </TextView>

    <TextView
        android:id="@+id/rs_strockexeco_tv7"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="線體:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv3"
        android:layout_alignParentLeft="true"
        ></TextView>
    <Spinner
        android:id="@+id/rs_strockexeco_linesp"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_floortv"
        android:layout_toEndOf="@+id/rs_strockexeco_tv7"
        >
    </Spinner>

    <TextView
        android:id="@+id/rs_strockexeco_tv4"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="部件編碼:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv7"
        android:layout_alignParentLeft="true"
        ></TextView>
    <TextView
        android:id="@+id/rs_strockexeco_productidtv"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text=""
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_linesp"
        android:layout_toEndOf="@+id/rs_strockexeco_tv4"
        >
    </TextView>

    <TextView
        android:id="@+id/rs_strockexeco_tv9"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="部件編號:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv4"
        android:layout_alignParentLeft="true"
        ></TextView>
    <TextView
        android:id="@+id/rs_strockexeco_productnametv"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text=""
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_productidtv"
        android:layout_toEndOf="@+id/rs_strockexeco_tv9"
        >
    </TextView>


    <TextView
        android:id="@+id/rs_strockexeco_tv8"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="原因編號:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv9"
        android:layout_alignParentLeft="true"
        ></TextView>
    <Spinner
        android:id="@+id/rs_strockexeco_reasonsp"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_productnametv"
        android:layout_toEndOf="@+id/rs_strockexeco_tv4"
        >
    </Spinner>

    <TextView
        android:id="@+id/rs_strockexeco_tv6"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="領用人:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv8"
        android:layout_alignParentLeft="true"
        ></TextView>
    <EditText
        android:id="@+id/rs_strocklist_execorequseret"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text=""
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_reasonsp"
        android:layout_toEndOf="@+id/rs_strockexeco_tv6"
        >
    </EditText>

    <TextView
        android:id="@+id/rs_strockexeco_tv5"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="出庫數量:"
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strockexeco_tv6"
        android:layout_alignParentLeft="true"
        ></TextView>
    <EditText
        android:id="@+id/rs_strocklist_execoqtyet"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text=""
        android:gravity="center_vertical"
        android:textSize="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/rs_strocklist_execorequseret"
        android:layout_toEndOf="@+id/rs_strockexeco_tv5"
        >
    </EditText>

    <Button
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:text="出庫"
        android:id="@+id/rs_strocklist_execobt"
        android:background="@drawable/btn_dl"
        android:textAlignment="center"
        android:textSize="15dp"
        android:layout_below="@+id/rs_strocklist_execorequseret"
        android:layout_toEndOf="@+id/rs_strocklist_execoqtyet"
        android:layout_marginLeft="15dp"
        android:onClick="submitoonclick"/>

</RelativeLayout>
