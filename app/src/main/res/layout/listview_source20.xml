        android:textColor="#000000"
        android:background="#b3ad9f"
        android:textAlignment="center"
        android:textSize="15sp"
        android:layout_toEndOf="@+id/ls8_Mdatetext1"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="1dp"
        />

    <TextView
        android:id="@+id/ls8_opname1"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:textAlignment="center"
        android:textColor="#000000"
        android:background="#abe9af"
        android:textSize="15sp"
        android:layout_toEndOf="@+id/ls8_userdept1"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="1dp"
        />

    <TextView
        android:id="@+id/ls8_linename1"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:textAlignment="center"
        android:textColor="#000000"
        android:background="#cfb863"
        android:textSize="15sp"
        android:layout_toEndOf="@+id/ls8_opname1"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="1dp"
        />
    <TextView
        android:id="@+id/ls8_checkmold"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:textColor="#000000"
        android:background="#c000ff00"
        android:textSize="15sp"
        android:textAlignment="center"
        android:layout_toEndOf="@+id/ls8_linename1"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="1dp"
        />
        <TextView
            android:id="@+id/ls8_fileversion"
            android:layout_width="0dp"
            android:layout_weight="1.2"
            android:layout_height="fill_parent"
            android:layout_toEndOf="@+id/ls8_linename1"
            android:background="#c4f9b2"
            android:textSize="15sp"
            android:textColor="#000000"
            android:textAlignment="center"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="1dp"
            />

